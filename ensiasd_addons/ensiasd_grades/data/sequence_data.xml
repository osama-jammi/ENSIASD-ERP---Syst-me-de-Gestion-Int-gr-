<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Séquence pour les délibérations -->
        <record id="seq_ensiasd_deliberation" model="ir.sequence">
            <field name="name">Délibération ENSIASD</field>
            <field name="code">ensiasd.deliberation</field>
            <field name="prefix">DEL/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les bulletins -->
        <record id="seq_ensiasd_bulletin" model="ir.sequence">
            <field name="name">Bulletin ENSIASD</field>
            <field name="code">ensiasd.bulletin</field>
            <field name="prefix">BUL/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les sessions -->
        <record id="seq_ensiasd_session" model="ir.sequence">
            <field name="name">Session ENSIASD</field>
            <field name="code">ensiasd.session</field>
            <field name="prefix">SES/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
